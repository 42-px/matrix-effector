{
    "presets": [
        [
            "@babel/preset-env",
            {
                "useBuiltIns": "entry",
                "corejs": 3,
                "modules": false,
                "shippedProposals": true
            }
        ]
    ],
    "plugins": [
        "effector/babel-plugin",
        "@babel/plugin-transform-runtime"
    ],
    "env": {
        "test": {
            "presets": [
                "@babel/preset-typescript"
            ],
            "plugins": [
                "@babel/plugin-transform-modules-commonjs"
            ]
        }
    }
}